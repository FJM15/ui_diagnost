<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">

    <s:String x:Key="AppTitle">Aplication Name</s:String>
	

	<s:String x:Key="PanelAutoManualMode">MANUAL,AUTOMATIC,SEMI AUTOMATIC,INDETERMINATED</s:String>
    <s:String x:Key="PanelAutoManualConfirmMessageManual">Confirm the change to Manual?</s:String>
    <s:String x:Key="PanelAutoManualConfirmMessageAuto">Confirm the change to Automatic?</s:String>

    <s:String x:Key="PanelAutoManualLabelManual">Manual</s:String>
    <s:String x:Key="PanelAutoManualLabelAuto">Automatic</s:String>
	
    <s:String x:Key="PanelStartStopMode">STOPPED AND READY,STARTED, FAULT OR ALARM,IN TRANSITION</s:String>
    <s:String x:Key="PanelStartStopConfirmMessageStart">Confirm the Start command?</s:String>
    <s:String x:Key="PanelStartStopConfirmMessageStop">Confirm the Stop command?</s:String>

    <s:String x:Key="PanelStartStopLabelStart">Start</s:String>
    <s:String x:Key="PanelStartStopLabelStop">Stop</s:String> 


</ResourceDictionary>